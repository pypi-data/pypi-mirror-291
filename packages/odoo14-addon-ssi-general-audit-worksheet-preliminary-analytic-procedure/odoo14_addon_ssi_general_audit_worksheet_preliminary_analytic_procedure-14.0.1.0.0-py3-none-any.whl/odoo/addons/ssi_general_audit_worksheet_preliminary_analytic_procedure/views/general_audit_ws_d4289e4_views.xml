<odoo>

    <record id="general_audit_ws_d4289e4_view_search" model="ir.ui.view">
        <field name="name">general_audit_ws_d4289e4 form</field>
        <field name="model">general_audit_ws_d4289e4</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_search"
        />
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <data />
        </field>
    </record>

    <record id="general_audit_ws_d4289e4_view_tree" model="ir.ui.view">
        <field name="name">general_audit_ws_d4289e4 tree</field>
        <field name="model">general_audit_ws_d4289e4</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_tree"
        />
        <field name="arch" type="xml">
            <data>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_d4289e4_view_form" model="ir.ui.view">
        <field name="name">general_audit_ws_d4289e4 form</field>
        <field name="model">general_audit_ws_d4289e4</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_form"
        />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//page[1]" position="before">
                    <page name="ratio" string="Ratios">
                        <button
                            name="action_compute_ratio"
                            type="object"
                            string="Compute"
                            icon="fa-calculator"
                            class="oe_highlight"
                        />
                        <field name="ratio_ids">
                            <tree
                                editable="top"
                                force_save="1"
                                delete="false"
                                create="false"
                            >
                                <field name="financial_ratio_id" />
                                <field name="category" />
                                <field
                                    name="extrapolation_amount"
                                    string="Extrapolation"
                                />
                                <field name="interim_amount" string="Interim" />
                                <field name="previous_amount" string="Previous" />
                                <field name="industry_average" />
                                <field name="analysis" />
                            </tree>
                        </field>
                    </page>
                </xpath>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_d4289e4_action" model="ir.actions.act_window">
        <field name="name">Preliminary Analytic Procedure - Ratio Analysis</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">general_audit_ws_d4289e4</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="general_audit_ws_d4289e4_menu"
        name="Preliminary Analytic Procedure - Ratio Analysis"
        parent="ssi_general_audit.menu_general_audit_risk_assesment"
        action="general_audit_ws_d4289e4_action"
        sequence="1402"
        groups="general_audit_ws_d4289e4_viewer_group"
    />


</odoo>
