        </field>
    </record>

    <record id="general_audit_ws_c8740d4_view_form" model="ir.ui.view">
        <field name="name">general_audit_ws_c8740d4 form</field>
        <field name="model">general_audit_ws_c8740d4</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_form"
        />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//page[1]" position="before">
                    <page
                        string="Preliminary Analytic Procedure Conclusion"
                        name="preliminary"
                    >
                        <field name="conclusion_ids">
                            <tree editable="top">
                                <field name="sequence" invisible="1" />
                                <field name="parent_sequence" invisible="1" />
                                <field name="category_id" />
                                <field name="name" />
                            </tree>
                        </field>
                    </page>
                </xpath>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_c8740d4_action" model="ir.actions.act_window">
        <field name="name">Preliminary Analytic Procedure</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">general_audit_ws_c8740d4</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="general_audit_ws_c8740d4_menu"
        name="Preliminary Analytic Procedure"
        parent="ssi_general_audit.menu_general_audit_risk_assesment"
